<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>Sign up</title>
  </head>
  <body>
    <form id="form1" method="post" action="/post-feedback">
        <div class="container" id="login">
            <br>
            <div class="row">
                <div class="col-md-2">&nbsp;</div>
                <div class="col-md-10">
                    <h2>Dang ky</h2>
                </div>
            </div>
            <div class="form-group row">
                <label for="name" class="col-md-2 col-form-label">Ho ten:</label>
                <div class="col-md-6">
                    <input type="text" class="form-control" id="name" name="name">
                </div>
                <div class="col-md-4">
                    <span style="color:red" id="er_name"></span>
                </div>
            </div>
            <div class="form-group row">
                <label for="birthday" class="col-md-2 col-form-label">Ngay sinh:</label>
                <div class="col-md-6">
                    <input type="text" class="form-control" id="birthday" name="birthday">
                </div>
                <div class="col-md-4">
                    <span style="color:red" id="er_birthday"></span>
                </div>
            </div>
            <div class="form-group row">
                <label for="sex" class="col-md-2 col-form-label">Gioi tinh:</label>
                <div class="col-md-auto">
                    <select id="sex" class="form-control">
                        <option>Nam</option>
                        <option>Nu</option>
                    </select>
                </div>
                <div class="col-md-8">&nbsp;</div>
            </div>
            <div class="form-group row">
                <label for="email" class="col-md-2 col-form-label">Email:</label>
                <div class="col-md-6">
                    <input type="text" class="form-control" id="email" name="email">
                </div>
                <div class="col-md-4">
                    <span style="color:red" id="er_email"></span>
                </div>
            </div>
            <div class="form-group row">
                <label for="username" class="col-md-2 col-form-label">Ten tai khoan:</label>
                <div class="col-md-6">
                    <input type="text" class="form-control" id="username" name="username">
                </div>
                <div class="col-md-4">
                    <span style="color:red" id="er_username"></span>
                </div>
            </div>
            <div class="form-group row">
                <label for="password" class="col-md-2 col-form-label">Mat khau:</label>
                <div class="col-md-6">
                    <input type="password" class="form-control" id="password" name="password">
                </div>
                <div class="col-md-4">
                    <span style="color:red" id="er_password"></span>
                </div>
            </div>
            <div class="form-group row">
                <label for="en_password" class="col-md-2 col-form-label">Nhap lai mat khau:</label>
                <div class="col-md-6">
                    <input type="password" class="form-control" id="en_password">
                </div>
                <div class="col-md-4">
                    <span style="color:red" id="er_en_password"></span>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md 2">&nbsp;</div>
                <div class="col-md-10">
                    <button type="button" id="chap_nhan" class="btn btn-primary">Chap nhan</button>
                    <a href="/"><button type="button" class="btn btn-outline-secondary">Quay lai</button></a>
                </div>
            </div>
        </div>
    </form>
      
  </body>
  <script src="jquery/jquery.min.js"></script>
  <script src="bootstrap/js/popper.min.js"></script>
  <script src="bootstrap/js/bootstrap.min.js"></script>
  <script src="js/js_signup.js"></script>
  <script src="js/appvue.js"></script>

</html>